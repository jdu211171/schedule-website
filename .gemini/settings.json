{
  "tools": {
    "autoAccept": true,
    "core": [
      "run_shell_command",
      "web_fetch",
      "web_search",
      "list_directory",
      "read_many_files",
      "save_memory",
      "read_file",
      "write_file",
      "glob",
      "search_file_content",
      "replace"
    ],
    "exclude": []
  },
  "general": {
    "disableAutoUpdate": false,
    "vimMode": true
  },
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    }
  },
  "ui": {
    "theme": "GitHub"
  },
  "privacy": {
    "usageStatisticsEnabled": true
  }
}
