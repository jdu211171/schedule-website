# Copy this file to .env.local and fill in your values

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/schedule_db"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-here"

# LINE Bot Configuration
# Get these from https://developers.line.biz/console/
LINE_CHANNEL_ACCESS_TOKEN="your-line-channel-access-token-here"
LINE_CHANNEL_SECRET="your-line-channel-secret-here"

# Cron Job Security
# Generate a random string for production
CRON_SECRET="your-cron-secret-here"

# Development/Testing
NODE_ENV="development"
NEXT_PUBLIC_URL="http://localhost:3000"